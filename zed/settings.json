{
  "buffer_font_family": "JetBrains Mono",
  "buffer_font_size": 13,
  "ui_font_family": ".SystemUIFont",
  "ui_font_size": 16,
  "theme": {
    "mode": "dark",
    "dark": "Catppuccin Macchiato",
    "light": "Catppuccin Latte"
  },
  "icon_theme": {
    "mode": "dark",
    "dark": "Catppuccin Macchiato",
    "light": "Catppuccin Latte"
  },
  "vim_mode": true,
  "vim": {
    "toggle_relative_line_numbers": true
  },
  "edit_predictions": {
    "mode": "subtle"
  },
  "project_panel": {
    "default_width": 320
  },
  "git_panel": {
    "default_width": 320
  },
  "collaboration_panel": {
    "default_width": 320
  },
  "outline_panel": {
    "default_width": 320
  },
  "tabs": {
    "git_status": true
  }
}
